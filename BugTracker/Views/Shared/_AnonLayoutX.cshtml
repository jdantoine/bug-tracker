
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>JD Bug Tracking System - by judner antoine</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Fontdiner+Swanky' rel='stylesheet' type='text/css'>
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />

</head>
<body class="p-t-0 m-a-0">
    <div class="container-fluid">
        <!-- BEGIN NAVBAR CONTENT-->
        <div class="navbar navbar-default" role="navigation">
            <div class="container-fluid">
                <!-- Navbar Header -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <i class="fa fa-angle-double-down"></i>
                    </button>
                </div>
                <!-- /navbar header -->
                <!-- Navbar Collapse -->
                <div class="navbar-collapse collapse">
                    <!-- left navbar -->
                    <ul class="nav navbar-nav top nav-pad">
                        <li><a href=@Url.Action("Index", "Home") class="text-title app-title">JD Bug Tracking System</a></li>
                    </ul>
                    <!-- right navbar -->
                    <ul class="nav navbar-nav navbar-right top-navbar nav-pad">

                        @*<li>@Html.Partial("_LoginPartial")</li>*@
                       
                        <li><a class="section-scroll" href=@Url.Action("Index", "Home")>Home</a></li>
                        <li><a class="section-scroll" href=@Url.Action("Contact", "Home")>Contact</a></li>
                        <li>@*<a class="section-scroll" href=@Url.Action("Contact", "Home")>Contact</a>*@</li>
                    
                    </ul>
                </div><!-- /.navbar-collapse -->
            </div><!-- /div .container-->
        </div>
        <!-- END NAVBAR CONTENT-->
        <section>
            @*<div class="body content rows scroll-y">*@
            @RenderBody()
            @*</div>*@
        </section>

        <!-- ABOUT SECTION -->
      




        <div class="clearfix"></div>

           
    </div>
    <footer>
        <p class="text-center" style="color:#262626">
            Created using ASP.NET MVC, C#, JS, Bootstrap, EF/SQL Server<br>
            &copy; 2017 - Judner Antoine
        </p>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
